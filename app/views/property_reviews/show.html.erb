<div class="page-header">
  <h3>Property Review #<%= @property_review.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Content</dt>
      <dd><%= @property_review.content %></dd>

      <dt>Property</dt>
      <dd><%= @property_review.property_id %></dd>

      <dt>User</dt>
      <dd><%= @property_review.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/property_reviews" class="btn btn-primary">
        Back
      </a>
      <a href="/property_reviews/<%= @property_review.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_property_review/<%= @property_review.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
